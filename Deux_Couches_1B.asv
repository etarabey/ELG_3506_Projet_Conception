syms n0 n1 n2 n3 R; 
g01 = (n0-n1)/(n0+n1); % gamma01
g12 = (n1-n2)/(n1+n2); % gamma12
g23 = (n2-n3)/(n2+n3); % gamma23

eqnR = (g01-g12-g01*g12*g23+g23)/(1-g01*g12-g12*g23+g01*g23).^2 == 0; 
